<%= render 'backend/shared/site_map' %>
<%= form_for([:backend, :interface, @interface_param], html:{ class: 'form-horizontal edit_style' })  do |f|%>

<%= render 'backend/shared/error', :locals => { :obj => @interface_param } %>

<%= bootstrap_text_field f, :param_name, '参数名称', '请输入参数名称' %>
<%= bootstrap_select f, :param_type, '参数类型', param_type_enum %>
<%= bootstrap_select f, :param_must, '是否必须参数', param_must_enum.map{ |key,value| [value, key] } %>
<%= bootstrap_text_field f, :param_desc, '参数描述', '请输入参数描述' %>

<%= bootstrap_commit_back_button(f, backend_interface_interface_params_path + append_param(params[:group_id])) %>

<%= hidden_field_tag 'group_id', params[:group_id] %>
<% end %>
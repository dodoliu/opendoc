<%= render 'backend/shared/site_map' %>
<%= form_for([:backend, :interface, @interface_response], html:{ class: 'form-horizontal edit_style' })  do |f|%>

<%= render 'backend/shared/error', :locals => { :obj => @interface_response } %>

<%= bootstrap_text_field f, :response_name, '参数名称', '请输入参数名称' %>
<%= bootstrap_select f, :response_type, '参数类型', param_type_enum %>
<%= bootstrap_text_field f, :response_desc, '描述', '请输入描述' %>

<%= bootstrap_commit_back_button(f, backend_interface_interface_responses_path + append_param(params[:group_id])) %>

<%= hidden_field_tag 'group_id', params[:group_id] %>
<% end %>
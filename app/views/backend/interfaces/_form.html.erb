<%= render 'backend/shared/site_map' %>
<%= form_for([:backend, :group, @interface], html:{ class: 'form-horizontal edit_style' })  do |f|%>

<%= render 'backend/shared/error', :locals => { :obj => @interface } %>


<%= bootstrap_text_field f, :interface_name, '接口名称', '请输入接口名称' %>
<%= bootstrap_text_field f, :domain, '域名', '请输入域名' %>
<%= bootstrap_text_field f, :product_url, '生产url', '请输入生产url' %>
<%= bootstrap_text_field f, :test_url, '测试url', '请输入测试url' %>
<%= bootstrap_select f, :request_type, '请求方式', request_type_enum %>
<%= bootstrap_select f, :response_type, '返回值格式', response_type_enum %>
<%= bootstrap_text_area f, :response_content, '返回结果' %>
<%= bootstrap_text_area f, :demo, 'demo' %>

<%= bootstrap_commit_back_button(f, backend_group_interfaces_path) %>

<% end %>
<%= render 'backend/shared/site_map' %>
<%= form_for([:backend, @member], html:{ class: 'form-horizontal edit_style' })  do |f|%>

<%= render 'backend/shared/error', :locals => { :obj => @member } %>

<%= bootstrap_text_field f, :display_name, '用户名称', '请输入用户名称' %>
<%= bootstrap_text_field f, :expire_date, '接口过期时间', '接口过期时间' %>
<%= bootstrap_text_field f, :memo, '备注', '请输入备注' %>

<%= bootstrap_commit_back_button(f, backend_members_path) %>


<% end %>